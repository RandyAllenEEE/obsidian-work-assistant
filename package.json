{
  "name": "work-assistant",
  "version": "2.0.0",
  "description": "A comprehensive productivity companion combining Calendar, Periodic Notes, and Word Count Heatmap features.",
  "author": "Liam Cain, Dhruvik Parikh, and Randy",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "lint": "svelte-check && eslint . --ext .ts",
    "build": "npm run lint && rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "lodash": "^4.17.21",
    "obsidian": "https://github.com/obsidianmd/obsidian-api.git#master",
    "obsidian-calendar-ui": "0.3.12",
    "obsidian-daily-notes-interface": "0.9.0",
    "sass": "^1.97.1",
    "svelte-writable-derived": "^3.1.1",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@tsconfig/svelte": "1.0.10",
    "@types/jest": "26.0.22",
    "@types/lodash": "^4.17.21",
    "@types/moment": "2.13.0",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "eslint": "7.23.0",
    "jest": "26.6.3",
    "moment": "2.29.1",
    "rollup": "^4.54.0",
    "rollup-plugin-svelte": "^7.2.3",
    "svelte": "^5.46.0",
    "svelte-check": "^4.3.5",
    "svelte-jester": "1.3.2",
    "svelte-preprocess": "^6.0.3",
    "ts-jest": "26.5.4",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  }
}